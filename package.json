    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit"
  },
  "license": "0BSD",
  "dependencies": {
    "@mdx-js/mdx": "v1",
    "@mdx-js/react": "v1",
